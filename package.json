{
  "name": "mg-dashboard",
  "version": "1.0.0",
  "description": "Dashboard for monitoring Mestinon treatment for Ocular Myasthenia Gravis",
  "main": "update-dashboard.js",
  "scripts": {
    "update": "node update-dashboard.js",
    "force-update": "node update-dashboard.js --force",
    "watch": "node update-dashboard.js --watch",
    "start": "npx http-server -o index.html",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src/**/*.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "myasthenia",
    "gravis",
    "mestinon",
    "pyridostigmin",
    "medical",
    "dashboard"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "d3": "^7.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@vitejs/plugin-legacy": "^7.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.47.0",
    "html-webpack-plugin": "^5.5.3",
    "http-server": "^14.1.1",
    "jsdom": "^26.1.0",
    "mini-css-extract-plugin": "^2.7.6",
    "style-loader": "^3.3.3",
    "vite": "^7.0.0",
    "vitest": "^3.2.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
